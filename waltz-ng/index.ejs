    <!--<% if (htmlWebpackPlugin.files.favicon) { %>-->
    <link rel="icon" href="<%=htmlWebpackPlugin.files.favicon%>">

    <!--<% } %>-->

    <script src="api/oauthdetails"></script>
    <style type="text/css">
        @keyframes waltz-splash-scale {
            0% { fill: #000 }
            50% { fill: #8e8e8d }
            100% { fill: #000 }
        }

        .waltz-splash-item {
            animation: waltz-splash-scale 1.5s ease infinite;
        }

        .waltz-splash-item-2 {
            animation-delay: .2s;
        }

        .waltz-splash-item-3 {
            animation-delay: .4s;
        }

        .waltz-splash-item-4 {
            animation-delay: .6s;
        }

        .waltz-splash-item-5 {
            animation-delay: .8s;
        }
    </style>

    <script type="text/javascript">
        (function() {
            // CustomEvent shim for IE11
            if (typeof window.CustomEvent === 'function') {
                // shim not required
                return false
            }

            function CustomEvent(event, params) {
                params = params || { bubbles: false, cancelable: false, detail: undefined }
                var evt = document.createEvent('CustomEvent')
                evt.initCustomEvent(event, params.bubbles, params.cancelable, params.detail)
                return evt
            }

            CustomEvent.prototype = window.Event.prototype

            window.CustomEvent = CustomEvent
        })();

        document.addEventListener("DOMContentLoaded", function(event) {
            var loadingElem = document.getElementById("page-loading");
            loadingElem.parentNode.removeChild(loadingElem);
            var appElem = document.getElementById("waltz-app");
            appElem.style.display = "block";
        });
    </script>

</head>

